{
  "name": "seatbelt-for-ghosts",
  "version": "1.0.0-alpha.0",
  "engines": {
    "node": ">=18.x"
  },
  "description": "Make governance safer",
  "scripts": {
    "compile": "hardhat compile",
    "start": "ts-node index.ts",
