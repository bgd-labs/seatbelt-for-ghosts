    "lint:fix": "prettier --write .",
    "test": "jest .",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bgd-labs/seatbelt-for-ghosts.git"
  },
  "keywords": [
    "aave",
    "governance",
    "safety"
  ],
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/bgd-labs/seatbelt-for-ghosts/issues"
  },
  "homepage": "https://github.com/bgd-labs/seatbelt-for-ghosts#readme",
  "dependencies": {
    "@actions/cache": "^3.0.0",
    "@bgd-labs/aave-address-book": "^1.24.0",
    "@bgd-labs/aave-cli": "^0.0.13",
    "@supercharge/promise-pool": "^2.3.0",
    "dotenv": "^16.0.1",
    "ethers": "^5.6.9",
    "gray-matter": "^4.0.3",
    "jest": "^28.1.2",
    "micro-ftch": "^0.3.1",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@types/jest": "^28.1.4",
    "@types/node": "^18.0.3",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.5"
